{
  "manifest_version": 3,
  "name": "burpbar",
  "version": "1.0.0",
  "description": "A basic example extension with only required keys",
  "content_scripts": [
    {
      "js": ["content-script.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "devtools_page": "devtools.html"
}
